{
  "name": "@motionpicture/mvtk-service",
  "version": "3.2.3-MWSR-278.alpha.0",
  "dependencies": {
    "debug": "^3.1.0",
    "soap": "0.13.0",
    "soap-cookie": "0.10.1"
  },
  "description": "Mvtk Client Library for Node.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "check": "npm run tslint",
    "test": "npm run check",
    "build": "tsc --project ./ --outDir lib --locale ja-jp",
    "tslint": "tslint --type-check --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" src/**/*.ts",
    "clean": "rimraf \"src/**/*.js ./lib test/**/*.js npm-debug.log* ./doc ./coverage ./.nyc_output\"",
    "doc": "jsdoc -c jsdoc.json",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/motionpicture/mvtk-service.git"
  },
  "keywords": [],
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "name": "Akito Hataguchi",
      "email": "hataguchi@motionpicture.jp"
    },
    {
      "name": "Tetsu Yamazaki",
      "email": "yamazaki@motionpicture.jp"
    }
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/soap": "^0.18.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "nsp": "^3.2.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.2",
    "typescript": "^2.7.2"
  }
}
